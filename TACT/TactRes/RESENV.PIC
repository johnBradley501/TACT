Box Dimen 6.5 in, 4 in
  [
    pgm:      box dimen 30%,30%
                  at (80%,30%)
              <
                  line {(0%,60%) (100%,60%)}
                  area dimen 100%, 40%
                  at (0%,60%)/tl
                  {text size 10 pt 'Res Modules' }
                  area dimen 100%, 60%
                  at (0%,0%)/tl
                  {text size 10 pt 'TACT/MAKBAS' }
              >

   resfile:   box dimen 25%,15%
                  at (xc pgm,75%)
                  < text size 10 pt 'RES File'>

              arrow {resfile.ct pgm.cb}

   resman:    box dimen 25%,20%
                  at (40%,yc resfile)
                  < text size 10 pt 'RESMAN'>

              arrow {resman.cr resfile.cl}

   modula:    box dimen 25%,20%
                  at (40%,yc pgm)
                  < text size 10 pt 'M2COMP' 'M2L'>

              arrow {modula.cr pgm.cl}

   pgmfiles:  box dimen 25%,15%
                  at (20%,53%)
                  < text size 10 pt 'PGM.MOD' >

              arrow {pgmfiles.cr corh modula.cb}
              arrow {pgmfiles.cr corh resman.ct}

]
